{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach (Node 7000)",
      "type": "node",
      "request": "attach",
      "port": 7000,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true,
      "restart": true,
      "continueOnAttach": true,
      "autoAttachChildProcesses": true
    },
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
    }
  ]
}
